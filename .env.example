# Hanzo IAM (hanzo.id) - OAuth2/OIDC Configuration
VITE_HANZO_IAM_URL=https://id.hanzo.ai
VITE_HANZO_CLIENT_ID=your-client-id
VITE_HANZO_REDIRECT_URI=http://localhost:8084/auth/callback

# Hanzo Commerce API
VITE_HANZO_API_URL=https://api.hanzo.ai

# Square Payment Processor (Primary)
VITE_SQUARE_APPLICATION_ID=sandbox-sq0idb-xxx
VITE_SQUARE_LOCATION_ID=your-location-id
# For production:
# VITE_SQUARE_APPLICATION_ID=sq0idp-xxx
# VITE_SQUARE_LOCATION_ID=your-production-location-id

# Crypto Payments
VITE_CRYPTO_DEPOSIT_ADDRESS=0x0000000000000000000000000000000000000000

# WalletConnect (for RainbowKit)
VITE_WALLETCONNECT_PROJECT_ID=your-project-id

# Optional RPC Endpoints (defaults to llamarpc.com public RPCs)
# VITE_RPC_MAINNET=https://eth-mainnet.g.alchemy.com/v2/xxx
# VITE_RPC_POLYGON=https://polygon-mainnet.g.alchemy.com/v2/xxx
# VITE_RPC_ARBITRUM=https://arb-mainnet.g.alchemy.com/v2/xxx
# VITE_RPC_OPTIMISM=https://opt-mainnet.g.alchemy.com/v2/xxx
# VITE_RPC_BASE=https://base-mainnet.g.alchemy.com/v2/xxx
# VITE_RPC_AVALANCHE=https://avalanche-mainnet.infura.io/v3/xxx
# VITE_RPC_BSC=https://bsc-dataseed.binance.org

# Optional: Enable additional payment processors
# VITE_STRIPE_ENABLED=false
# VITE_PAYPAL_ENABLED=false
